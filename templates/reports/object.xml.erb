<document name="report">
 <ns2:reports_common xmlns:ns2="http://collectionspace.org/services/report">
   <forDocTypes>
<% data[:fordoctypes].split(",").map(&:strip).each do |doc_type| %>
    <forDocType><%= doc_type %></forDocType>
<% end %>
   </forDocTypes>
   <name><%= data[:name] %></name>
   <notes><%= data[:notes] %></notes>
   <supportsSingleDoc><%= data[:supportssingledoc] %></supportsSingleDoc>
   <supportsDocList><%= data[:supportsdoclist] %></supportsDocList>
   <supportsGroup><%= data[:supportsgroup] %></supportsGroup>
   <supportsNoContext><%= data[:supportsnocontext] %></supportsNoContext>
   <filename><%= data[:filename] %></filename>
   <outputMIME><%= data[:outputmime] %></outputMIME>
 </ns2:reports_common>
</document>