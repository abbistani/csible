<?xml version="1.0" encoding="UTF-8"?>
<document name="persons">
  <ns2:persons_common xmlns:ns2="http://collectionspace.org/services/person" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <refName>urn:cspace:cspace.lyrasistechnology.org:personauthorities:name(person):item:name(<%= data[:shortidentifier] %>)'<%= data[:name] %>'</refName>
    <shortIdentifier><%= data[:shortidentifier] %></shortIdentifier>
    <personTermGroupList>
      <personTermGroup>
        <termDisplayName><%= data[:name] %></termDisplayName>
        <foreName><%= data[:first] %></foreName>
        <middleName><%= data[:middle] %></middleName>
        <surName><%= data[:last] %></surName>
      </personTermGroup>
    </personTermGroupList>
  </ns2:persons_common>
</document>